{
	"name": "base-js-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"commit": "cz",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"eslint:check": "eslint .",
		"lint": "npm run eslint:check && npm run prettier:check",
		"lint:fix": "npm run eslint:check && npm run prettier:fix",
		"prepare": "husky install",
		"dev": "next dev",
		"build": "next build",
		"start": "next start"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{html,md,json,yml,yaml,js,css,scss}": "npm run lint:fix"
	},
	"prettier": "./configs/formatting/.prettierrc.json",
	"eslintConfig": {
		"extends": [
			"./configs/linting/.eslintrc.json"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.2",
		"@commitlint/config-conventional": "^17.7.0",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.51.0",
		"eslint-plugin-react": "^7.33.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"prettier": "^2.8.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Bekert/project-base.git"
	},
	"author": "Bekert",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Bekert/project-base/issues"
	},
	"homepage": "https://github.com/Bekert/project-base",
	"dependencies": {
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.13",
		"@mui/joy": "^5.0.0-beta.10",
		"@mui/material": "^5.14.13",
		"eslint-config-next": "^13.5.4",
		"next": "^13.5.4",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-responsive-carousel": "^3.2.23"
	}
}
